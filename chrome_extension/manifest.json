{
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "ui/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["scripts/content.js"]
    }
  ],
  "description" : "An architectural template of a Chrome extension.",
  "manifest_version": 2,
  "name": "Chrome Extension Boilerplate",
  "permissions": [],
  "version": "0.1"
}